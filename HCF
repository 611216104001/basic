int_lst = list(map(int,input().split()))[:2]
if(int_lst[0] % int_lst[1] == 0) or (int_lst[1] % int_lst[0] == 0):
    if(int_lst[0] < int_lst[1]):
        print(int_lst[0])
    else:
        print(int_lst[1])
else:
   if(int_lst[0] < int_lst[1]):
        hcf = (int_lst[1] % int_lst[0])
        print(hcf)
   else:
        hcf = (int_lst[0] % int_lst[1])
        print(hcf)
